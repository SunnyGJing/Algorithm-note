//不对哦！！！看书！ 学到了结构体的输入与输出以及赋值
#include <cstdio>
struct student {
	char name[10];
	char Id[10];
	int score ;
}students[100];
int main(){
	int n,s,max=0, min=0, maxI, minI;
	char c;
	scanf("%d",&n);
	for(int i=0; i<n; i++){ 
		int j = 0,k = 0; 
		while((c=getchar())!=' '){
		students[i].name[j++] = c; 
		}
		while(c=getchar()!=' '){
		students[i].Id[k++] = c; 
		}
		scanf("%d",&s);
		students[i].score = s;
		if(max<s){
			max=students[i].score;
			maxI = i;
		}
		else if(min>s){
			min=students[i].score;
			minI = i;
		} 
	}
	printf("%s",students[maxI].name);
	printf(" ");
	printf("%s",students[maxI].Id);
	printf("\n");
	printf("%s",students[minI].name);
	printf(" ");
	printf("%s",students[minI].Id);
	return 0;
} 
