class Solution1 {
public:
    vector<int> printListFromTailToHead(ListNode* head) {
        vector<int> result;
        while(head != NULL) {
            result.push_back(head->val);
            head = head->next;
        }
        reverse(result.begin(), result.end());
        
        return result;
    }
};


class Solution2 {
public:
    vector<int> result;
    vector<int> printListFromTailToHead(ListNode* head) {
        if(head == NULL) return result;
        printListFromTailToHead(head->next);
        result.push_back(head->val);
        return result;
    }
};